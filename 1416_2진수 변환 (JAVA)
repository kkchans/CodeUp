import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner  scan = new Scanner(System.in);
		int num1 = scan.nextInt();
		int num = num1;
		int ba[] = new int[50];
		int i = 0;
		while(num!=0) {
			ba[i]=num%2;
			num/=2;
			i++;
		}
		if(num1==0) {
		    System.out.print(num1);
		}else {
		for(int j = i-1 ; j >= 0; j--) {
			System.out.print(ba[j]);
		}
		}
	}

}
