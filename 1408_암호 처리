#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	char a[20];
	scanf("%s", a);
	for (int i = 0; a[i] != NULL; i++) {
		int c = (int)a[i]+2;
		char d = (char)c;
		printf("%c", d);
	}
	printf("\n");
	for (int i = 0; a[i] != NULL; i++) {
		int c = (int)a[i]*7%80+48;
		char d = (char)c;
		printf("%c", d);
	}
}
