#include <iostream>
using namespace std;
int main() {
	int n, j;
	cin >> n;
	int br[100];
	for (int i = 0; i < n; i++) {
		cin >> br[i];
	}
	for (int i = 0; i < n; i++) {
		for (j = i; j < n; j++) {
			cout << br[j] << " ";
		}
		for (int k = 0; k < i; k++) {
			cout << br[k] << " ";
		}
		cout << endl;
	}
	return 0;
}
